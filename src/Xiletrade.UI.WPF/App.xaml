<Application x:Class="Xiletrade.UI.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:md="http://schemas.lepo.co/wpfui/2022/xaml/markdown"
             xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ui:ThemesDictionary Theme="Dark" />
                <ui:ControlsDictionary />
                <md:ThemesDictionary Theme="Dark" />
                <md:ControlsDictionary />
                <ResourceDictionary Source="Themes\Converter.xaml" />
                <ResourceDictionary Source="Themes\Media.xaml" />
                <ResourceDictionary Source="Themes\Image.xaml" />
                <ResourceDictionary Source="Themes\Window.xaml" />
                <ResourceDictionary Source="Themes\Control.xaml" />
                <ResourceDictionary Source="Themes\UserControl.xaml" />
                <ResourceDictionary Source="Themes\TextBlock.xaml" />
                <ResourceDictionary Source="Themes\TextBox.xaml" />
                <ResourceDictionary Source="Themes\Combobox.xaml" />
                <ResourceDictionary Source="Themes\ToolTip.xaml" />
                <ResourceDictionary Source="Themes\Border.xaml" />
                <ResourceDictionary Source="Themes\Button.xaml" />
                <ResourceDictionary Source="Themes\CheckBox.xaml" />
                <ResourceDictionary Source="Themes\ListView.xaml" />
                <ResourceDictionary Source="Themes\ListBox.xaml" />
                <ResourceDictionary Source="Themes\Label.xaml" />
                <ResourceDictionary Source="Themes\StackPanel.xaml" />
                <ResourceDictionary Source="Themes\Menu.xaml" />
                <ResourceDictionary Source="Themes\TabControl.xaml" />
                <ResourceDictionary Source="Themes\ScrollViewer.xaml" />
                <ResourceDictionary Source="Themes\Slider.xaml" />
                <ResourceDictionary Source="Themes\SliderFlat.xaml" />
                <!--<ResourceDictionary Source="Themes\SliderMod.xaml" />-->
                <ResourceDictionary Source="Themes\DataGrid.xaml" />
                <!--<ResourceDictionary Source="Themes\Styles.xaml" />-->
                <ResourceDictionary Source="Themes\ToggleButton.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <FontFamily x:Key="LithosProRegular">pack://application;,,,/Assets/Lithos-Pro-Regular.ttf#Lithos Pro Regular</FontFamily>
            <FontFamily x:Key="FontinSmallCaps">pack://application;,,,/Assets/Fontin-SmallCaps.ttf#Fontin SmallCaps</FontFamily>
            <FontFamily x:Key="FontinItalic">pack://application;,,,/Assets/Fontin-Italic.ttf#Fontin Italic</FontFamily>
            <FontFamily x:Key="OptimusPrincepsSemiBold">pack://application;,,,/Assets/OptimusPrincepsSemiBold.ttf#Optimus Princeps Semi Bold</FontFamily>
            <FontFamily x:Key="FrizQuadrataC">pack://application;,,,/Assets/FrizQuadrataC.ttf#Friz Quadrata C</FontFamily>
            <!--
            <FontFamily x:Key="NotoSansJPRegular">pack://application;,,,/Assets/NotoSans-jp-42-Regular.ttf#NotoSans JP Regular</FontFamily>
            <FontFamily x:Key="CinzelRegular">pack://application;,,,/Assets/Cinzel-Regular.ttf#Cinzel Regular</FontFamily>
            <FontFamily x:Key="CaveatBold">pack://application;,,,/Assets/Caveat-Bold.ttf#Caveat Bold</FontFamily>
            <FontFamily x:Key="Quikhand">pack://application;,,,/Assets/Quikhand.ttf#Quikhand</FontFamily>
            <FontFamily x:Key="Daehan">pack://application;,,,/Assets/Korean-DaehanA1.ttf#Daehan</FontFamily>
            -->
        </ResourceDictionary>
    </Application.Resources>
</Application>
